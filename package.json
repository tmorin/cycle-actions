{
  "name": "@tmorin/cycle-actions",
  "version": "0.3.0",
  "description": "A Cycle.js driver to manage actions.",
  "keywords": [
    "cycle",
    "cyclejs",
    "cycle.js",
    "driver"
  ],
  "main": "lib/cjs/driver.js",
  "module": "lib/es6/driver.js",
  "typings": "lib/cjs/driver.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/tmorin/cycle-actions.git"
  },
  "author": {
    "name": "Thibault Morin",
    "url": "https://tmorin.github.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/tmorin/cycle-actions/issues"
  },
  "homepage": "https://gitlab.com/tmorin/cycle-actions#readme",
  "scripts": {
    "transpile": "npm run transpile-cjs && npm run transpile-es6",
    "transpile-cjs": "tsc --module commonjs --outDir ./lib/cjs",
    "transpile-es6": "tsc --module es6 --outDir ./lib/es6",
    "test": "JEST_JUNIT_OUTPUT='./coverage/junit.xml' jest --coverage --ci --reporters=default --reporters=jest-junit",
    "test-watch": "jest --watchAll",
    "typedoc": "typedoc src",
    "release": "standard-version"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true
  },
  "peerDependencies": {
    "xstream": "*",
    "@cycle/run": "*"
  },
  "devDependencies": {
    "@cycle/isolate": "^5.0.0",
    "@cycle/run": "^5.2.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2",
    "xstream": "^11.11.0"
  }
}
